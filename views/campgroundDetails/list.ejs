<% layout('layouts/style') %> 
<div id="cluster-map"></div>
    <h1>All_CampGround</h1>
    <a href="/campground/new">Create A new Campground</a>
    <% for(let camp of allCamp) {%> 
        <div class="card mb-3" >
            <div class="row">
                <div class="col-md-4">
                    <% if(camp.images.length) {%> 
                    <img class="img-fluid" src="<%= camp.images[0].url %>" alt="" >
                    <% }else{ %> 
                        <img class="img-fluid" src="https://res.cloudinary.com/daqedg8xh/image/upload/v1668522211/Arghya_YelpCamp/wpi7bua1hfdccs3pf7va.jpg" alt="" >
                        <% } %> 
                </div>
                <div class="col-md-8">
                    <h5 class="card-title">
                        <%= camp.title %>
                    </h5>
                    
                    <p class="card-text">
                        <%= camp.description %>
                    </p>
                    <p class="card-text">
                        <small class="text-muted">
                            <%= camp.location %>
                        </small>
                    </p>
                    <a class="btn btn-primary" href="/campground/<%= camp._id %>">View <%= camp.title %> </a>
                </div>
            </div>
        </div>      
    <% } %> 
    <a href="/campground/new">Create A new Campground</a>


    <script>
        let mapToken = '<%-process.env.MAPBOX_TOKEN%>'
